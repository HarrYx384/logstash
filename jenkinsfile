pipeline {
    agent any
    stages {
        stage ("code checkout"){
            steps{
                git branch: 'main', credentialsId: 'hudson', url: 'https://github.com/HarrYx384/logstash.git'
            }
        }
        stage("now trigger packer"){
            steps{
                sh 'mkdir now'
                sh 'packer build /var/lib/jenkins/workspace/logstash/packer.json'
            }
        }
    }
}
